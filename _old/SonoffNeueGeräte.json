var AllDevicesLast, ObjectID, AllDevices;

// Beschreibe diese Funktion …
async function GetName(ObjectID) {
    return GlobalGetName(ObjectID);
}


AllDevicesLast = [];
AllDevices = Array.prototype.slice.apply($("channel[state.id=sonoff.*.*.alive]"));
on({id: [].concat(AllDevices), change: "ne"}, async function (obj) {
  let value = obj.state.val;
  let oldValue = obj.oldState.val;
  if (AllDevices.length != AllDevicesLast.length) {
    console.log((['Neues Sonoff Gerät: ',obj.id,' - ',await GetName(obj.id)].join('')));
    setState("0_userdata.0.GlobalVars.TelegramSystem"/*TelegramSystem*/, (['Neues Sonoff Gerät: ',obj.id,' - ',await GetName(obj.id)].join('')));
    AllDevicesLast = AllDevices;
  }
});

