var Intervall, result;

function listsGetRandomItem(list, remove) {
  var x = Math.floor(Math.random() * list.length);
  if (remove) {
    return list.splice(x, 1)[0];
  } else {
    return list[x];
  }
}


Intervall = setInterval(async function () {
  exec('ls /opt/iobroker/iobroker-data/files/vis.0/screensaver/* | sed  -e \'s/$/;/\' -e \'s/\\/opt\\/iobroker\\/iobroker-data\\/files//\'|tr --delete \'\\n\'', async function (error, result, stderr) {
      setStateDelayed('0_userdata.0.GlobalVars.screensaver_img', (listsGetRandomItem(result.split(';'), false)), false, parseInt(((0) || "").toString(), 10), false);
  });
  console.log("exec: " + 'ls /opt/iobroker/iobroker-data/files/vis.0/screensaver/* | sed  -e \'s/$/;/\' -e \'s/\\/opt\\/iobroker\\/iobroker-data\\/files//\'|tr --delete \'\\n\'');
}, 30000);

