var text, Intervall;

// Beschreibe diese Funktion …
async function speak(text) {
  if (getState('0_userdata.0.GlobalVars.AtHome').val && getState('0_userdata.0.GlobalVars.AlarmTimerState').val) {
    setState('alexa2.0.Echo-Devices.G0922H0843620RFD.Commands.speak-volume' /* Volume to use for speak commands */, 100);
    setState('alexa2.0.Echo-Devices.G0922H0843620RFD.Commands.speak' /* speak */, text);
  }
}


