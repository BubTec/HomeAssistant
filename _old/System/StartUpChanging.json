var timeout, ChangeDP;

// Beschreibe diese Funktion â€¦
async function Changeit() {
  ChangeDP = '0_userdata.0.StartUpChanging';
  if (getState(ChangeDP).val) {
    console.log('startupchanging FALSE');
    setStateDelayed(ChangeDP, false, false, parseInt(((0) || "").toString(), 10), false);
  } else {
    console.log('startupchanging TRUE');
    setStateDelayed(ChangeDP, true, false, parseInt(((0) || "").toString(), 10), false);
  }
}


timeout = setTimeout(async function () {
  await Changeit();
}, 30000);

schedule("0 2 * * *", async function () {
  await Changeit();
});

