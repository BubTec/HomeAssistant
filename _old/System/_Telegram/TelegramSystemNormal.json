var instance, message, mute, repeats, long, lat;

// Beschreibe diese Funktion …
async function telegram(instance, message, mute) {
    message = GlobalRemoveHTMLCode (message);
    sendTo('telegram.' + instance, 'send', {text: message, disable_notification: mute});

}

// Beschreibe diese Funktion …
async function telegramCall(message, repeats) {
    sendTo('telegram.0', 'call', {
        text: message,
        language: 'de-DE-Standard-A',
        repeats: repeats
    });

}

// Beschreibe diese Funktion …
async function telegramPosition(long, lat) {
    sendTo('telegram.0', {
        latitude:               lat,
        longitude:              long,
        disable_notification:   false
    });
}


on({id: [].concat(['0_userdata.0.GlobalVars.TelegramNormal']), change: "any"}, async function (obj) {
  let value = obj.state.val;
  let oldValue = obj.oldState.val;
  console.log('TelegramNormal');
  await telegram('0', (obj.state ? obj.state.val : ""), false);
});
