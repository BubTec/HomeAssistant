var CatFood, CatFoodMessage, CatFoodIntervall;

function listsGetRandomItem(list, remove) {
  var x = Math.floor(Math.random() * list.length);
  if (remove) {
    return list.splice(x, 1)[0];
  } else {
    return list[x];
  }
}

// Beschreibe diese Funktion …
async function KatzeFuettern() {
  CatFoodMessage = listsGetRandomItem(['Achtung. Barbarossa muss noch gefüttert werden.', 'Eine Nachricht von Barbarossa:<break time="1s"/> MIAUU! <break time="500ms"/> Bitte gebt mir Futter. Ich habe Hunger!', 'Eine Nachricht von Barbarossa:<break time="1s"/> MIAUU! <break time="500ms"/> Los meine Sklaven. Füttert euren Herren!'], false);
  setStateDelayed('0_userdata.0.GlobalVars.AlexaNotify', { val: CatFoodMessage, ack: false }, parseInt(((0) || '').toString(), 10), false);
}


CatFood = '0_userdata.0.Rooms.Küche.CatFood';
schedule("0 16 * * *", async () => {
  setStateDelayed(CatFood, { val: false, ack: false }, parseInt(((0) || '').toString(), 10), false);
});
on({ id: [].concat(CatFood), change: 'ne' }, async (obj) => {
  let value = obj.state.val;
  let oldValue = obj.oldState.val;
  if ((obj.state ? obj.state.val : '')) {
    (() => { if (CatFoodIntervall) { clearInterval(CatFoodIntervall); CatFoodIntervall = null; }})();
  } else {
    await KatzeFuettern();
    CatFoodIntervall = setInterval(async () => {
      await KatzeFuettern();
    }, 1800000);
  }
});

